#php_flag display_errors off
Options -Indexes
#indexIgnore *

RewriteEngine On
#ErrorDocument 404 /404.html
ErrorDocument 404 Maaf-halaman-tidak-ditemukan

	#refrensi
	RewriteRule   ^home/?$   									index.php?page=vhome
	RewriteRule   ^tahun-buku/?$   								index.php?page=tahunbuku
	RewriteRule   ^rekening/?$   								index.php?page=rekening
	RewriteRule   ^tahun-anggaran/?$   							index.php?page=tahun_anggaran
	
	#kesiswaan
	RewriteRule   ^transaksi/?$			   						index.php?page=transaksi
	RewriteRule   ^modul-pembayaran/?$   						index.php?page=pembayaran
	RewriteRule   ^pembayaran-pendaftaran/?$					index.php?page=pembayaran_pendaftaran
	RewriteRule   ^pembayaran-uang-pangkal/?$					index.php?page=uang_pangkal
	RewriteRule   ^pembayaran-uang-sekolah/?$					index.php?page=uang_sekolah
	RewriteRule   ^inventory/?$									index.php?page=inventory
	
	RewriteRule   ^keluar/?$ 									index.php?page=logout 

# Send all blocked request to homepage with 403 Forbidden error!
#RewriteRule ^(.*)$ index.php [F,L]